{
    "AWSTemplateFormatVersion": "2010-09-09",
    "Parameters": {
        "authferjekaiUserPoolId": {
            "Type": "String",
            "Default": "authferjekaiUserPoolId"
        },
        
        "authferjekaiAppClientID": {
            "Type": "String",
            "Default": "authferjekaiAppClientID"
        },
        "authferjekaiAppClientIDWeb": {
            "Type": "String",
            "Default": "authferjekaiAppClientIDWeb"
        },
        "AuthRoleArn": {
            "Type": "String"
        },
        "UnauthRoleArn": {
            "Type": "String"
        },
        "env": {
            "Type": "String"
        }
    },
    "Conditions": {
        "ShouldNotCreateEnvResources": {
            "Fn::Equals": [
                {
                    "Ref": "env"
                },
                "NONE"
            ]
        }
    },
    "Resources": {
    
        "adminsGroup": {
          "Type" : "AWS::Cognito::UserPoolGroup",
          "Properties" : {
              "GroupName" : "admins",
              "Precedence" : 1,
              
              "UserPoolId" : {
                "Ref": "authferjekaiUserPoolId"
              }
            }
        },
        
    
    
    },
    
}
